{
  "name": "challange-solarview",
  "version": "0.0.1",
  "description": "Challange project for job position at SolarView",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/drivers/api/server.js",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "check": "npm-check -s -u",
    "lint": "eslint 'src/**' --fix",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/drivers/api/server.ts",
    "build:watch": "npm run clean && tsc -p tsconfig-build.json -w",
    "debug": "nodemon -L --watch dist --inspect=0.0.0.0:9222 --nolazy dist/drivers/api/server.js",
    "on-docker": "npm run migration:up && npm run seed:up && npm start",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "migration:createdb": "sequelize-cli db:create",
    "migration:status": "sequelize-cli db:migrate:status",
    "migration:create": "sequelize-cli migration:generate --name",
    "migration:up": "sequelize-cli db:migrate",
    "migration:down": "sequelize-cli db:migrate:undo",
    "seed:create": "sequelize-cli seed:generate --name",
    "seed:up": "sequelize-cli db:seed:all",
    "seed:down": "sequelize-cli db:seed:undo:all",
    "test": "jest --passWithNoTests --runInBand --no-cache --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand --forceExit",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "graphql-iso-date": "^3.6.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "6.6.2",
    "sequelize-typescript": "^2.1.0",
    "sequelize-cli": "^6.2.0",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "@databases/mysql-test": "^3.1.1",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.4.13",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/umzug": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "apollo-server-integration-testing": "^3.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.2.5",
    "jest": "^27.0.6",
    "jest-mysql": "^1.1.6",
    "lint-staged": "^10.5.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/snxtecnologia/challenge-solarview.git"
  },
  "keywords": [
    "challange",
    "nodejs",
    "graphql",
    "sequelize",
    "mysql"
  ],
  "author": "leandro@snxtecnologia.com",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/snxtecnologia/challenge-solarview#readme"
}
